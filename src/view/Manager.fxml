<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
   </right>
   <center>
      <VBox prefHeight="200.0" prefWidth="299.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Label text="Process ID:" />
                  <ComboBox prefHeight="13.0" prefWidth="93.0" />
               </children>
            </HBox>
            <TextField maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="175.0" promptText="Enter Process Size Numerically">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TextField>
            <ComboBox prefWidth="150.0" visibleRowCount="3" />
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" text="Run" />
                  <Button mnemonicParsing="false" text="Compact" />
                  <Button mnemonicParsing="false" text="Remove" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
   <bottom>
      <Label text="Status Label" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
